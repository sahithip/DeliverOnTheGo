<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.finalproject.deliveronthego.SignUp"
    android:id="@+id/textView3">

    <TextView android:text="@string/AppName" android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView3" />

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:weightSum="1"
        android:id="@+id/signUPLayout">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"></FrameLayout>

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="50dp"
            android:layout_marginRight="10dp"
            android:id="@+id/signUpFrame">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="First Name"
                android:id="@+id/textView5"
                android:layout_gravity="left|top"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Last Name"
                android:id="@+id/textView7"
                android:layout_gravity="left|top"
                android:layout_marginTop="60dp"
                android:layout_marginLeft="10dp" />

            <EditText
                android:layout_width="210dp"
                android:layout_height="wrap_content"
                android:id="@+id/editText"
                android:layout_gravity="right|top"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="10dp"
                android:singleLine="true"
                android:visibility="visible" />

            <EditText
                android:layout_width="210dp"
                android:layout_height="wrap_content"
                android:id="@+id/editText5"
                android:layout_gravity="right|top"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="60dp"
                android:singleLine="true"
                android:visibility="visible" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Email ID"
                android:id="@+id/textView8"
                android:layout_gravity="left|top"
                android:layout_marginTop="130dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="160dp" />

            <EditText
                android:layout_width="210dp"
                android:layout_height="wrap_content"
                android:id="@+id/editText4"
                android:layout_gravity="right|top"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="130dp"
                android:singleLine="true"
                android:visibility="visible" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Password"
                android:id="@+id/textView6"
                android:layout_gravity="left|top"
                android:layout_marginTop="210dp"
                android:layout_marginLeft="10dp" />

            <EditText
                android:layout_width="210dp"
                android:layout_height="wrap_content"
                android:id="@+id/editText6"
                android:layout_gravity="right|top"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="210dp"
                android:singleLine="true"
                android:visibility="visible" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Phone Number"
                android:id="@+id/textView9"
                android:layout_gravity="left|top"
                android:layout_marginTop="270dp"
                android:layout_marginLeft="10dp" />

            <EditText
                android:layout_width="210dp"
                android:layout_height="wrap_content"
                android:id="@+id/editText7"
                android:layout_gravity="right|top"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="270dp"
                android:singleLine="true"
                android:visibility="visible" />

            <Button
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sign Up as User"
                android:id="@+id/button"
                android:layout_marginTop="320dp"
                android:layout_marginLeft="100dp"
                android:onClick="signUpUser" />

        </FrameLayout>

    </LinearLayout>

</RelativeLayout>
